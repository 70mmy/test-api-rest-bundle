{
    "name": "everycheck/test-api-rest-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily test a rest api",
    "keywords": [
        "test",
        "api",
        "rest"
    ],
    "homepage": "https://bitbucket.org/everycheck/test-api-rest-bundle",
    "license": "MIT",
    "require": {
        "phpunit/phpunit": "^6.0",
        "symfony/phpunit-bridge": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "fzaninotto/faker": "^1.6",
        "nelmio/alice": "^3.0@beta",
        "canadadry/php-matcher": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "EveryCheck\\TestApiRestBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/",
            "/Doc/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryCheck\\TestApiRestBundle\\Tests\\": "Tests"
        }
    }
}